{
	"extends": "airbnb-base",
	"parser": "@babel/eslint-parser",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"import/no-extraneous-dependencies": [
			"error",
			{
				"devDependencies": [
					"gulpfile.babel.js",
					"gulp/**/*.js"
				]
			}
		],
		"linebreak-style": 0,
		"indent": [
			"error",
			"tab"
		],
		"no-tabs": 0,
		"implicit-arrow-linebreak": 0,
		"object-curly-newline": 0,
		"no-console": 0,
		"no-restricted-syntax":"off",
		"prefer-arrow-callback": "off",
		"func-names": "off",
		"no-plusplus": "off",
		"no-use-before-define": "off"
	}
}